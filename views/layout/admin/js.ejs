<script>
    function toggleDropdownTyper() {
        const dropdownContentCoupon = document.getElementById("dropdown-content-typer");
        dropdownContentCoupon.style.display = dropdownContentCoupon.style.display === "none" ? "block" : "none";
    }
    
    function toggleDropdownUser() {
        const dropdownContentUser = document.getElementById("dropdown-content-user");
        dropdownContentUser.style.display = dropdownContentUser.style.display === "none" ? "block" : "none";
    }
   
    
      </script>