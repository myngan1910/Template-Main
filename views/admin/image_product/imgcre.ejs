<!doctype html>
<html lang="en">
  <%- include('../../layout/admin/header'); -%>
  <body>
    <div class="container-fluid">
        <div class="row vh-100">
          <%- include('../../layout/admin/sidebar'); -%>
            <div class="col-lg-9  ">
                <div class="row text-center">
                    <h3>Create Image</h3> 
                 
                </div>
                
                <form action="/admin/createimg" method="post" enctype="multipart/form-data">
                  <div class="form-group">
                     
                    <label for="image">Image:</label>
                    <img> <input type="file"  name="image" multiple /><br>
                    <label for="productid">Productid:</label>
                      <select type="text" class="form-control" value="" name="productid">
                        <% for(var i=0; i< productid.length;i++){%>
                           <option VALUE="<%=productid[i].id %>"><%=productid[i].id %></option>
                       <%}%>
                         </select>
                    

                      
                      
                      
                      
                    </div>
                  <button type="submit" class="btn btn-dark">Create</button>
                </form>
                 
            </div>
        </div>
    </div>
    
  </body>
</html>