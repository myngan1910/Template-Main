<!doctype html>
<html lang="en">
  <%- include('../../layout/admin/header'); -%>
  <body>
    <div class="container-fluid">
        <div class="row vh-100">
          <%- include('../../layout/admin/sidebar'); -%>
            <div class="col-lg-9  ">
                <div class="row text-center">
                    <h2>Contact</h2> 
                      
                    <table class="table">
                      <thead>
                        <tr>
                          <th>ID</th>
                          <th>Name</th>
                          <th>Subject</th>
                          <th>Mail</th>
                          <th>Phone</th>
                          <th>Message</th>
                          
                         
                         
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var i =0 ; i < data.length; i++) {%>
                        <tr>
                          <td><%= data[i].id %></td>
                          <td><%= data[i].name %></td>
                          <td><%= data[i].subject %></td>
                          <td><%= data[i].mail %></td>
                          <td><%= data[i].phone %></td>
                          <td><%= data[i].message %></td>
                          
                          <td><a href="/admin/ctc/<%= data[i].id %>">View
                            /<a href="/admin/ctcdele/<%= data[i].id %>">Delete</a></td>
                  
                        </tr>
                        <% }%>
                        
                      </tbody>
                    </table>
                
          
                </div>
                
            </div>
            
          </div>
  </body>
</html>